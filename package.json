{
	"name": "@egjs/view3d",
	"version": "1.1.0-snapshot",
	"description": "Fast & customizable 3D model viewer for everyone based on three.js & typescript",
	"main": "dist/view3d.js",
	"module": "dist/view3d.esm.js",
	"es2015": "dist/view3d.esm.js",
	"types": "declaration/index.d.ts",
	"scripts": {
		"dev": "rollup -w --config ./rollup.config.dev.js",
		"build": "rm -rf ./dist ./declaration && rollup -c && npm run declaration && npm run printsizes",
		"declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json",
		"printsizes": "print-sizes ./dist --exclude=\\.map",
		"lint": "eslint 'src/**/*.ts'",
		"lint:test": "eslint 'test/unit/**/*.ts'",
		"docs:build": "jsdoc-to-mdx -c ./jsdoc-to-mdx.json",
		"demo:build": "npm run build && cpx 'dist/**/*' demo/release/latest/dist --clean",
		"demo:prebuild-version": "cpx 'dist/**/*' demo/release/$npm_package_version/dist --clean && cpx 'docs/**/*' demo/release/$npm_package_version/docs --clean",
		"demo:prebuild-latest": "cpx 'dist/**/*' demo/release/latest/dist --clean && cpx 'docs/**/*' demo/release/latest/docs --clean",
		"demo:deploy-origin": "npm run build && npm run jsdoc && npm run demo:prebuild-version && npm run demo:prebuild-latest && gh-pages -d demo/",
		"demo:deploy": "npm run build && npm run jsdoc && npm run demo:prebuild-version && npm run demo:prebuild-latest && gh-pages -d demo/ --add --remote upstream",
		"release": "release-helper upstream"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/naver/egjs-view3d"
	},
	"keywords": [
		"model-viewer",
		"3d",
		"three",
		"3d-model",
		"typescript"
	],
	"author": "NAVER Corp.",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/naver/egjs-view3d/issues"
	},
	"homepage": "https://github.com/naver/egjs-view3d#readme",
	"dependencies": {
		"@egjs/component": "^3.0.2",
		"three": "~0.134.0"
	},
	"devDependencies": {
		"@babel/plugin-proposal-class-properties": "^7.8.3",
		"@babel/plugin-transform-object-assign": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.16.5",
		"@babel/preset-env": "^7.12.11",
		"@babel/runtime": "^7.16.5",
		"@daybrush/jsdoc": "^0.3.7",
		"@egjs/release-helper": "0.0.3",
		"@rollup/plugin-commonjs": "^11.1.0",
		"@rollup/plugin-node-resolve": "^7.1.3",
		"@rollup/plugin-replace": "^2.3.2",
		"@types/node": "^14.14.35",
		"@types/resize-observer-browser": "^0.1.6",
		"@types/three": "^0.134.0",
		"@types/webxr": "^0.2.3",
		"@typescript-eslint/eslint-plugin": "^4.18.0",
		"@typescript-eslint/eslint-plugin-tslint": "^4.18.0",
		"@typescript-eslint/parser": "^4.18.0",
		"babel-plugin-no-side-effect-class-properties": "0.0.7",
		"concurrently": "^5.1.0",
		"cpx": "^1.5.0",
		"eslint": "^7.22.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsdoc": "^30.7.9",
		"eslint-plugin-prefer-arrow": "^1.2.3",
		"fs-extra": "^10.0.0",
		"gh-pages": "2.0.1",
		"husky": "^4.2.1",
		"jsdoc-to-mdx": "^1.1.0",
		"print-sizes": "^0.1.0",
		"rollup": "^1.10.1",
		"rollup-plugin-babel": "^4.4.0",
		"rollup-plugin-livereload": "^1.3.0",
		"rollup-plugin-prototype-minify": "^1.1.0",
		"rollup-plugin-serve": "^1.0.1",
		"rollup-plugin-typescript2": "^0.27.0",
		"rollup-plugin-uglify": "^6.0.4",
		"rollup-plugin-visualizer": "^4.0.4",
		"tslint": "^6.1.3",
		"typescript": "~3.8.0"
	},
	"husky": {
		"hooks": {
			"commit-msg": "node config/validate-commit-msg.js"
		}
	}
}
