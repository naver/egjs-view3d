{
  "name": "@egjs/vue3-view3d",
  "version": "2.3.1",
  "description": "Fast & Customizable glTF 3D model viewer, packed with full of features!",
  "main": "dist/view3d.cjs.js",
  "module": "dist/view3d.esm.js",
  "types": "declaration/index.d.ts",
  "sideEffects": [
    "**/*.css",
    "**/*.sass"
  ],
  "files": [
    "./declaration/**/*",
    "./dist/**/*",
    "./sass/**/*",
    "./css/**/*",
    "./src/**/*",
    "./README.md",
    "./LICENSE"
  ],
  "scripts": {
    "start": "vite --host 0.0.0.0 --port 3000",
    "build:vue": "vue-tsc && vite build",
    "build": "rm -rf dist && rollup -c && npm run declaration",
    "postbuild": "cpx '../view3d/css/*' css/ && cpx '../view3d/sass/*' sass/",
    "declaration": "rm -rf declaration && tsc -p tsconfig.declaration.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/naver/egjs-view3d/tree/master/packages/vue3-view3d"
  },
  "author": {
    "name": "NAVER Corp."
  },
  "homepage": "https://naver.github.io/egjs-view3d",
  "bugs": {
    "url": "https://github.com/naver/egjs-view3d/issues"
  },
  "license": "MIT",
  "browserslist": [
    "last 2 version",
    "ie 9-11",
    "not op_mini all"
  ],
  "dependencies": {
    "@egjs/view3d": "~2.10.1"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/runtime-dom": "^3.2.47",
    "rollup-plugin-vue": "^6.0.0",
    "tslib": "^2.3.1",
    "vite": "^4.1.0",
    "vue": "^3.2.47",
    "vue-router": "^4.0.12",
    "vue-tsc": "^1.0.24"
  },
  "keywords": [
    "3D",
    "3D-model",
    "model-viewer",
    "three",
    "webgl",
    "canvas",
    "typescript"
  ]
}
